    "eslint-import-resolver-typescript": "3.5.2",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jest": "27.1.4",
    "eslint-plugin-prettier": "4.2.1",
    "husky": "8.0.1",
    "jest": "29.2.2",
    "jest-mock": "29.2.2",
    "lint-staged": "13.0.3",
